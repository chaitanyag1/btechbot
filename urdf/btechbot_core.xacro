<?xml version="1.0"?>
<robot xmlns:xacro="http://www.ros.org/wiki/xacro" name="btechbot">


 <link name = "base_link">
 </link>

 <joint name = "chassis_joint" type = "fixed">
   <parent link = "base_link" />
   <child link = "chassis" />
   <origin xyz = "0 0 0" />
 </joint>

 <!--Chassis Link-->

  <link name = "chassis">
    <visual>
      <geometry>
        <box size = "0.22 0.18 0.003"/>
      </geometry>
      <origin xyz = "0 0 0"/>
      <material name = "jute_fibre">
        <color rgba = "0.5 0.25 0.0 1.0"/>
