    </visual>
    <collision>
      <geometry>
        <box size = "0.22 0.18 0.003"/>
      </geometry>
      <origin xyz = "0 0 0"/>
    </collision>
    <inertial>
      <mass value = "0.05" />
      <origin xyz = "0 0 0" rpy = "0 0 0" />
      <inertia ixx = "0.001953" ixz = "0" ixy="0" iyy = "0.01386" iyz = "0" izz = "0.014456" />
    </inertial>
  </link>
  
