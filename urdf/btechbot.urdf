<?xml version="1.0"?>

<robot name = "btechbot">

 <!--Base-link-->

 <link name = "base_link">
 </link>

 <!--joint-Base-chassis-->

 <joint name = "chassis_joint" type = "fixed">
   <parent link = "base_link" />
   <child link = "chassis" />
   <origin xyz = "0 0 0" />
 </joint>

 <!--Chassis Link-->

  <link name = "chassis">
    <visual>
      <geometry>
        <box size = "0.22 0.18 0.003"/>
      </geometry>
      <origin xyz = "0 0 0"/>
      <material name = "jute_fibre">
        <color rgba = "0.5 0.25 0.0 1.0"/>
      </material>
    </visual>
    <collision>
      <geometry>
        <box size = "0.22 0.18 0.003"/>
      </geometry>
      <origin xyz = "0 0 0"/>
    </collision>
    <inertial>
      <mass value = "0.05" />
      <origin xyz = "0 0 0" rpy = "0 0 0" />
      <inertia ixx = "0.001953" ixz = "0" ixy="0" iyy = "0.01386" iyz = "0" izz = "0.014456" />
    </inertial>
  </link>
  
  <gazebo reference="chassis">
    <material>Gazebo/Orange</material>
  </gazebo>

  <!-- Left Wheel -->

  <link name = "left_wheel">
    <visual>
      <geometry>
        <cylinder radius = "0.022" length = "0.018" />
      </geometry>
      <material name = "black tire">
        <color rgba = "0.1 0.1 0.1 1.0"/>
      </material>
   </visual>
   <collision>
      <geometry>
        <cylinder radius = "0.022" length = "0.018" />
      </geometry>
   </collision>
   <inertial>
      <mass value = "0.018" />
      <origin xyz = "0 0 0" rpy = "0 0 0" />
      <inertia ixx = "0.0002" ixz = "0" ixy="0" iyy = "0.0002" iyz = "0" izz = "0.00004" />
    </inertial>
  </link>

  <gazebo reference="left_wheel">
    <material>Gazebo/Black</material>
  </gazebo>
  
  <joint name = "lwheel" type="continuous">
    <parent link = "base_link"/>
    <child link = "left_wheel"/>
    <origin xyz = "0 0.099 0" rpy = "1.587 0 0"/>
    <axis xyz = "0 0 -1"/>
  </joint>

 <!-- Right Wheel -->

  <link name = "right_wheel">
    <visual>
      <geometry>
        <cylinder radius = "0.022" length = "0.018" />
      </geometry>
      <material name = "black tire">
        <color rgba = "0.1 0.1 0.1 1.0"/>
      </material>
     </visual>
     <collision>
      <geometry>
        <cylinder radius = "0.022" length = "0.018" />
      </geometry>
     </collision>
     <inertial>
      <mass value = "0.018" />
      <origin xyz = "0 0 0" rpy = "0 0 0" />
      <inertia ixx = "0.0002" ixz = "0" ixy="0" iyy = "0.0002" iyz = "0" izz = "0.00004" />
    </inertial>
   </link>
   
   <gazebo reference="right_wheel">
    <material>Gazebo/Black</material>
  </gazebo>

  <joint name = "rwheel" type="continuous">
    <parent link = "base_link"/>
    <child link = "right_wheel"/>
    <origin xyz = "0 -0.099 0" rpy = "-1.587 0 0"/>
    <axis xyz = "0 0 1"/>
  </joint>

 <!-- Front Caster wheel -->

 <joint name = "fcaster" type = "fixed">
